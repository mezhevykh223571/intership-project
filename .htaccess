<Files xmlrpc.php>
Order Deny,Allow
Deny from all
</Files>

RedirectMatch 404 /\.git
# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /intership-project/
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /intership-project/index.php [L]
</IfModule>

# END WordPress